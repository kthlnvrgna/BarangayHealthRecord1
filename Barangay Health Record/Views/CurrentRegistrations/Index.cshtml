@model IEnumerable<LogicLayer.PatientsModel>

@{
    ViewBag.Title = "Index";
} 
<h5>Registrations as of @DateTime.Now.ToShortDateString() </h5> 
<table class="table" style="text-align:center">
    <tr> 
        <th> 
            <span>Check up Number:</span>
        </th>
        <th>
            <span>First Name</span>
        </th>
        <th>
            <span>Middle Name</span>
        </th>
        <th>
            <span>Last Name</span>
        </th>
        <th>
            <span>Sex</span>
        </th>
        <th>
            <span>Patient Type</span>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr> 
    <td>
        @Html.DisplayFor(modelItem => item.RegNum)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.FirstName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MiddleName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.LastName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Sex)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Category)
    </td>
    <td>
        @Html.ActionLink("Details", "Details", "CurrentRegistrations", new { id = -1, regnum = item.RegNum }, null)
    </td>
</tr>
}

</table>
